<?xml version="1.0" encoding="utf-8"?>
<transition:EffectContainerAS xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx"
		  xmlns:transition="com.iyoya.components.container.photo.setting.transition.*">
	
	<s:VGroup>
		<s:BorderContainer id="borderContainer"
						   skinClass="spark.skins.spark.BorderContainerSkin"
						   borderColor="0x222222"
						   borderWeight="2"
						   width="85"
						   height="64">
			<s:Image id="effectImg"
					 width="100%"
					 height="100%"/>
		</s:BorderContainer>
		<s:Label id="effectTitle"
				 width="85"
				 textAlign="center"
				 text="[{indexStr}] {title}"/>
	</s:VGroup>
</transition:EffectContainerAS>
