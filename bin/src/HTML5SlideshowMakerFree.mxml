					   showStatusBar="false"
					   skinClass="com.anvsoft.skins.WindowedApplicationSkin"
					   xmlns:container="com.anvsoft.components.container.*"
					   xmlns:header="com.anvsoft.components.header.*"
					   xmlns:footer="com.anvsoft.components.footer.*"
					   frameRate="30"
					   preinitialize="new AppMain(this)">
	
	<fx:Style source="kingnarestyle.css"/>
	<fx:Style source="defaults.css"/>
	<s:layout>
		<s:VerticalLayout gap="0"/>
